{
  "name": "lish-survey-backend",
  "version": "1.0.0",
  "description": "LISH AI LABS Policy Survey Backend API",
  "main": "index.js",
  "scripts": {
    "build": "npm run db:generate && npm run build:frontend",
    "build:backend": "tsc",
    "build:frontend": "cd frontend && npm install && npm run build",
    "dev": "concurrently \"npm run dev:api\" \"npm run dev:frontend\"",
    "dev:api": "vercel dev --listen 3000",
    "dev:frontend": "cd frontend && npm run dev",
    "start:local": "npm run db:generate",
    "dev:watch": "nodemon",
    "deploy": "vercel --prod",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:studio": "prisma studio",
    "db:seed": "ts-node prisma/seed.ts",
    "setup:admin": "node scripts/setup-admin.js",
    "test": "jest",
    "test:api": "jest --testPathPattern=api",
    "test:watch": "jest --watch",
    "test:manual": "node scripts/test-api.js",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix"
  },
  "dependencies": {
    "@prisma/client": "^5.7.1",
    "@vercel/node": "^3.0.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "csv-writer": "^1.6.0",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "rate-limiter-flexible": "^4.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.0",
    "@types/supertest": "^2.0.16",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "axios": "^1.6.2",
    "concurrently": "^9.2.1",
    "eslint": "^8.54.0",
    "express": "^4.18.2",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prisma": "^5.7.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2",
    "vercel": "^33.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "survey",
    "api",
    "vercel",
    "prisma",
    "postgresql"
  ],
  "author": "LISH AI LABS",
  "license": "MIT"
}
